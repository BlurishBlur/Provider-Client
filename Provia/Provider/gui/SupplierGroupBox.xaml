<Page x:Class="Provider.gui.SupplierGroupBox"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Provider.gui"
      mc:Ignorable="d" 
      d:DesignHeight="499" d:DesignWidth="626"
      Title="Page">

    <Grid Background="White">
        <GroupBox x:Name="groupBox" Header="Beskrivelse" HorizontalAlignment="Left" Margin="20,20,0,0" VerticalAlignment="Top" Height="154" Width="586" FontFamily="Yu Gothic UI" FontSize="16">
            <TextBlock x:Name="textBlock" HorizontalAlignment="Left" TextWrapping="Wrap" Text="Lorem ipsum dolor sit amet, consectetur adipiscing elit. Curabitur libero tortor, eleifend at mollis eget, convallis a nisi. Curabitur leo lorem, eleifend et orci et, semper feugiat dolor. Suspendisse fringilla sagittis ex sit amet tempor. Mauris venenatis, elit in sagittis convallis, ex sapien suscipit neque, vel eleifend elit arcu facilisis tellus. Proin vestibulum convallis tortor, ac suscipit lectus fringilla vel. Phasellus eu lacinia dui, et gravida mi. Phasellus tincidunt bibendum justo. Pellentesque sed interdum est, vel auctor felis. Fusce ac libero aliquam, accumsan lacus ullamcorper, tempus lorem. In hac habitasse platea dictumst. Mauris scelerisque, urna nec gravida cursus, dolor ante suscipit nisl, ut dictum lorem ligula quis magna. Nullam auctor ante urna, quis hendrerit magna congue vitae. Phasellus tempus hendrerit diam posuere congue. Praesent id risus quis nibh tincidunt elementum. Quisque sed placerat elit, quis tristique metus." VerticalAlignment="Top" Height="130" Width="560" Margin="10,0,0,-4" FontSize="12" TextTrimming="WordEllipsis"/>
        </GroupBox>
        <GroupBox x:Name="groupBox1" Header="Lokation" HorizontalAlignment="Left" Margin="20,185,0,0" VerticalAlignment="Top" Height="60" Width="280" FontFamily="Yu Gothic UI" FontSize="16">
            <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="10,0,0,0" TextWrapping="Wrap" Text="Lorem ipsum dolor sit amet, consectetur adipiscing elit." VerticalAlignment="Top" FontSize="12" TextTrimming="CharacterEllipsis" Width="254"/>
        </GroupBox>
        <GroupBox x:Name="groupBox1_Copy" Header="Kontaktinformation" HorizontalAlignment="Left" Margin="326,185,0,0" VerticalAlignment="Top" Height="60" Width="280" FontFamily="Yu Gothic UI" FontSize="16">
            <TextBlock x:Name="textBlock1_Copy" HorizontalAlignment="Left" Margin="8,0,0,0" TextWrapping="Wrap" Text="Lorem ipsum dolor sit amet, consectetur adipiscing elit." VerticalAlignment="Top" FontSize="12" TextTrimming="CharacterEllipsis" Width="256"/>
        </GroupBox>
        <GroupBox x:Name="groupBox_Copy" Header="Note" HorizontalAlignment="Left" Margin="20,256,0,0" VerticalAlignment="Top" Height="154" Width="586" FontFamily="Yu Gothic UI" FontSize="16">
            <TextBox x:Name="noteTextBox" HorizontalAlignment="Left" Height="129" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="576" Margin="0,0,-2,-3" BorderBrush="{x:Null}" FontSize="12" IsReadOnly="True"/>
        </GroupBox>
        <Button x:Name="editNote" Content="Rediger Notater" HorizontalAlignment="Left" Margin="20,424,0,0" VerticalAlignment="Top" Width="112" Click="EditNote"/>
    </Grid>
</Page>
